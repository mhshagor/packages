{   
    "name": "mhshagor/packages",
    "description": "Package - Beautiful package for managing packages",
    "keywords": [
        "laravel",
        "package"
    ],
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mhshagor\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhshagor\\Package\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhshagor\\Package\\Providers\\PackagesServiceProvider"
            ]
        }
    },    
    "authors": [
        {
            "name": "M.H SHAGOR",
            "email": "srq001100@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}